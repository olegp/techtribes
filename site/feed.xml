---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Techtribes - Tech Events in Finland</title>
    <description>Active tech community events and meetups in Finland</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ site.url }}{{ site.baseurl }}/feed.xml" rel="self" type="application/rss+xml" />
    <language>en</language>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    {% assign sorted_events = site.data.output.events | sort: "isoDate" | reverse %}
    {% for item in sorted_events %}
    <item>
      <title>{{ item.name | xml_escape }}</title>
      <link>{{ item.event }}</link>
      <guid>{{ item.event }}</guid>
      {% assign date_parts = item.date | split: "/" %}
      {% assign iso_date = date_parts[2] | append: "-" | append: date_parts[1] | append: "-" | append: date_parts[0] %}
      <pubDate>{{ iso_date | date_to_rfc822 }}</pubDate>
    </item>
    {% endfor %}
  </channel>
</rss>
